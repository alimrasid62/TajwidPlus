<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#D3A300"/>
            <corners android:radius="12dp"/>
            <padding android:left="16dp"
                android:top="10dp"
                android:right="16dp"
                android:bottom="10dp"/>
            <stroke android:width="2dp" android:color="#FAE397"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#D5C48C"/>
            <corners android:radius="12dp"/>
            <padding android:left="16dp"
                android:top="10dp"
                android:right="16dp"
                android:bottom="10dp"/>
            <stroke android:width="2dp" android:color="#746532"/>
        </shape>
    </item>
</selector>
